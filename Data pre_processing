for i in range(0, len(parquet_files), batch_size):\n","    batch = parquet_files[i:i + batch_size]\n","    batch_dfs = []\n","\n","    for f in batch:\n","        print(f\"Loading {f} ...\")\n","        df = cudf.read_parquet(f)\n","        batch_dfs.append(df)\n","\n","    combined = cudf.concat(batch_dfs, ignore_index=True)\n","\n","    # Save batch in chunks\n","    batch_file = os.path.join(batch_output_dir, f\"fhvhv_batch_{i // batch_size}.parquet\")\n","    print(f\"Saving intermediate batch to {batch_file} ...\")\n","\n","    # Convert the cuDF dataframe to a PyArrow Table\n","    table = pa.Table.from_pandas(combined.to_pandas())\n","\n","    # Write the first chunk to create the file\n","    pq.write_table(table, batch_file)\n","\n","    # === Free GPU and CPU memory ===\n","    del combined, batch_dfs, df, table\n","    cupy.get_default_memory_pool().free_all_blocks()\n","    gc.collect()\n","\n","print(\"✅ All batches processed and saved.\")"],"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"collapsed":true,"id":"w8lZbgAe1anf","executionInfo":{"status":"ok","timestamp":1745968891544,"user_tz":420,"elapsed":3316742,"user":{"displayName":"Tin Phan","userId":"11573120625890018946"}},"outputId":"115e854e-e25f-46b1-f19f-216567f70d19"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Loading /content/drive/MyDrive/fhvhv_trip_data/fhvhv_tripdata_2020-01.parquet ...\n","Loading /content/drive/MyDrive/fhvhv_trip_data/fhvhv_tripdata_2020-02.parquet ...\n","Saving intermediate batch to /content/drive/MyDrive/trip_batches/fhvhv_batch_0.parquet ...\n","Loading /content/drive/MyDrive/fhvhv_trip_data/fhvhv_tripdata_2020-03.parquet ...\n","Loading /content/drive/MyDrive/fhvhv_trip_data/fhvhv_tripdata_2020-04.parquet ...\n","Saving intermediate batch to /content/drive/MyDrive/trip_batches/fhvhv_batch_1.parquet ...\n","Loading /content/drive/MyDrive/fhvhv_trip_data/fhvhv_tripdata_2020-05.parquet ...\n","Loadin